{
    "installCommands":"npm install",
    "version":2,
    "builds":[
        {
            "src":"index.js",
            "use":"@versel/node"
        },
        {
            "src":"src/**/*",
            "use":"@versel/static"
        }
    ],
    "routes":[
        { "src":"/(.*)","dest":"/" }
    ]
}